#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main(){
	ll t;
	cin >> t;
	while(t--){
		ll n;
		cin >> n;
		vector<vector<ll>> v;
		for(int i = 0; i < n; i++){
			ll l;
			cin >> l;
			vector<ll> vv(l);
			for(int j = 0; j < l; j++) cin >> vv[i];
			sort(vv.begin(), vv.end());
			v.push_back(vv);
		}
		ll cnt = n;
		sort(v.begin(), v.end());
		vector<int> p1(n, 1), p2(n, 1);
		for(int i = 0; i < n; i++){
			bool flag = false;
			for(int j = 0; j < v[i].size(); j++){
				if(p1[j]){
					p1[j] = 0;
					p2[i] = 0;
					flag = true;
					cnt--;
				}
			}
			if(flag) continue;
		}
		if(cnt == 0){
			cout << "OPTIMAL" << endl;
			continue;
		}
		cout << "IMPROVE" << endl;
		for(int i = 0; i < n; i++){
			if(p1[i] != 0) cout << p1[i] << " ";
		}
		for(int j = 0; j < n; j++) if(p2[j] != 0) cout << p2[j]<< endl;
	}
    return 0;
}

