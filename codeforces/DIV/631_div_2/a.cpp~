
#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
    ll t;
    cin >> t;
    while(t--){
        ll n, x;
        cin >> n >> x;
        vector<int> v(x+n+ 1);
        for(int i = 0; i < n; i++){
            int k; 
            cin >> k;
            v[k++] = 1;
        }
        int i = 1, cnt = 0;
        while(v[i]){ 
            cnt++;
            i++;
        }

        cout << cnt << " ans " << endl << endl;
    }
    return 0;
}
