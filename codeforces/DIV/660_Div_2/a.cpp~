#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll t;
    cin >> t;
    while(t--){
        ll n;
        cin >> n;
        n -= 30;
        int flag = 0;
        if(n > 0){
            if( n == 6){
                cout << "5 6 10 15" << endl;
            }else{
                if(n == 10){
                    cout << "6 10 21 3" << endl;
                }else{
                    if(n == 14){
                        cout << "6 7 10 21" << endl;
                    }else{
                        cout << "6 10 14 " << n << endl;
                    }
                }
            }
        }
        else cout << "NO" << endl;
    }
    return 0;
}
