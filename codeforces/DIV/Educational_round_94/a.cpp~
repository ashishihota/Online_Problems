#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll t;
    cin >> t;
    while(t--){
        ll n, o = 0, z = 0; 
        cin >> n;
        string s;
        cin >> s;
        for(int i = 0; i < s.length(); i++){
            if(s[i] == '0') z++;
            else o++;
        }
       // cout << "values of o ans 1 are " << z << " " << o << endl;
        string ans = "";
        if(z > o){
            ans.insert(ans.begin(), n, '0');
        }else ans.insert(ans.begin(), n, '1');
        cout << ans << endl;
    }
    return 0;
}
