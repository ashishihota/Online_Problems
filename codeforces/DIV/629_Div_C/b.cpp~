#include<bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

void bin(unsigned n)
{
    if (n > 1)
    bin(n>>1);

    printf("%d", n & 1);
}


int main() {
    ll t;
    cin >> t;
    while(t--){
        ll n , k;
        cin >> n >> k;
        ll tot = (n*(n-1)) /2;
        string s = "";
        for(int i =0; i < n; i++){
            s += "a";
        }
        bool flag  = false;
        ll fi = 2, se = 1;
        while(tot <= k){
            tot += se;
            se++;
        }
        cout << se + 1 << " " << se << " tot = " << tot;
        //cout << s;
        cout << endl;
    }
    return 0;
}
