#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll t;
    cin >> t;
    while(t--){
        ll n;
        cin >> n;
        vector<int> a(100), b(100);
        for(int i = 0; i < n; i++){
            int v;
            cin >> v;
            if(a[v] == 0){
                a[v]++;
            }
            else b[v]++;
        }
        int ans = 0;
        bool first = false, second = false;
        for(int i = 0; i < 100; i++){
            if(first && second) break;
            if( !first ){
                if(a[i] == 0){
                    ans += i;
                    first = true;
                }
            }
            if(!second){
                if(b[i] == 0){
                    ans += i;
                    second = true;
                }
            }
        }
        cout << ans << endl;
    }
    return 0;
}
