#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll t;
    cin >> t;
    while(t--){
        ll n;
        cin >> n;
        vector<int> v(n);
        for(int i = 0; i < n; i++){
            cin >> v[i];
        }
        ll f,s,sum,third = 0;
        f = v[0]; s = v[1]; sum = f+s;
        for(int i = 2; i < n; i++){
            if(v[i] >= sum){
                third = i;
                break;
            }
        }
        if(third != 0){
            cout << 1 << " " << 2 << " " << third+1<<endl;
        }else cout << "-1" <<endl;

    }
    return 0;
}
