
#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {

    ll t;
    cin >>t;
    while(t--){
        ll a, b, c, d, u = 0, v= 0;
        cin >> a >> b >> c >> d;
        ll x, y, xf, xs, yf, ys;
        cin >> x >>y >> xf >> yf >> xs >> ys;
        x  -= a; x += b;
        y -= c; y += d;
        if(a - b + c - d < 1){
            cout << "NO" << endl;
            continue;
        }
        if((x >= xf) && (x <= xs) && (y >= yf) && (y <= ys)) cout << "YES" << endl;
        else cout << "NO" << endl;
    }
    return 0;
}
