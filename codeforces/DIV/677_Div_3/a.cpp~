#define __USE_MINGW_ANSI_STUDIO 0
#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    vector<int> v;
    for(int i = 1; i <= 9; i++){
        v.push_back(i);
        v.push_back(10 * i + 1);
        v.push_back(100 * i + 10 * i + i);
        v.push_back(1000 * i + 100 * i + 10 * i + i);
    }
    while(t--){
        ll n, cnt = 0 ;
        cin >> n;
        for(int i = 0; v[i] <= n; i++){
            cnt += to_string(v[i]).length();
        }
        cout << cnt << endl;
    }
}
