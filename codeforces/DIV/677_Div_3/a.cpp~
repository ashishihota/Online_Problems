#include <bits/stdc++.h>

using namespace std;


typedef long long ll;
typedef pair<int, int> pii;
const int MOD = 1e9 + 7;

#define rep(i,b) for(i=0;i<b;++i)
#define repp(i,a,b) for(i=a;i<b;++i)

int main() {
	ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while(t--){
        ll n;
        cin >> n;
        int i = 11111;
        int cnt = 0;
        while(i > n){
           i /= 10;
        }
        int ans = 0;
        int rem  = n / i;
        if(i == 1111){
            ans += rem * 4;
            ans += 54;
        }
        else if( i == 111){
            ans += rem * 3;
            ans += 27;
        }
        else if( i == 11){
            ans += rem * 2;
            ans += 9;
        }
        else{
            ans += rem;
        }
        cout << ans << endl;
     }

    return 0;
}
